                    <select name="flip-1" class="nCompletionLocationFlip" data-role="slider" data-mini="true">
                        <option value="off">Off</option>
                        <option value="on">On</option>
                    </select>
                </div>
                -->
                    
                    <div class="nCompletionInput nCompletionInputLocationContainer">
                        <asp:DropDownList ID="DropDownListCountry" runat="server" AutoPostBack="False" OnSelectedIndexChanged="DropDownListCountry_SelectedIndexChanged" data-role="none" CssClass="nCompletionLocationInput nCompletionLocationInputCountry nCompletionTextInput">
                        </asp:DropDownList>
                    </div>
                    <div class="clear"></div>
                    <div class="nCompletionInput nCompletionInputLocationContainer">
                        <asp:DropDownList ID="DropDownListCity" runat="server" data-role="none" CssClass="nCompletionLocationInput nCompletionLocationLastInput">
                            <asp:ListItem Value="0">Select City</asp:ListItem>
                        </asp:DropDownList>
                    </div>
                    <div class="clear"></div>
                    <asp:TextBox ID="TextBoxRequestLocationCountry" CssClass="nCompletionNewCountry invisible" placeholder="country" runat="server" data-role="none"></asp:TextBox>
                    <div class="clear"></div>
                    <asp:TextBox ID="TextBoxRequestLocationCity" CssClass="nCompletionNewCity invisible" placeholder="city" runat="server" data-role="none"></asp:TextBox>
                    <div class="nCompletionNotListedButton nSearchChange">Not listed yet? Click here!</div>
                    <div class="nCompletionListedButton nSearchChange invisible">Go back to lists</div>
                </div>
            </div>
        </div>
        <div class="nCompletionValidationErrorMessage invisible nCompletionLocationError">Please choose your location</div>
        <div class="nCompletionValidationErrorMessage invisible nCompletionNewLocationError">Please enter new locations properly</div>
        <div class="nCompletionValidationErrorMessage nCompletionValidationErrorMessageBackEnd  nCompletionUNError invisible">&nbsp<asp:Label ID="LabelMessage" runat="server" Visible="false"></asp:Label></div>
        <br />
        <div class="nCompletionErrorMessage"></div>
        <div class="invisible">
