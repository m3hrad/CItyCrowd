                <div class="nCompletionName">Last Name</div>
                <div class="nCompletionArrowLN nSIRowEnterArrow nCompletionArrow nValidatorButton invisible"></div>
                <div class="nCompletionStatus"></div>
                <div class="nCompletionInput">
                    <asp:TextBox ID="TextBoxLastName" runat="server" CssClass="nCompletionTextInput nCompletionTextInputLN" data-role="none"></asp:TextBox>
                </div>
            </div>
