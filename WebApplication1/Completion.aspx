            <asp:HiddenField ID="HiddenFieldHasPhoto" runat="server" Value="false" />
            <asp:HiddenField ID="HiddenFieldPhotoUrl" runat="server" />
            <asp:HiddenField ID="HiddenFieldLocationStatus" runat="server" Value="1" />
